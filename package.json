{
  "name": "hrrrr",
  "version": "0.0.1",
  "description": "Hapi, React, Redux, React Router",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmenshov/hrrrr.git"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",

    "test": "npm run test:jest && npm run test:nightwatch",
